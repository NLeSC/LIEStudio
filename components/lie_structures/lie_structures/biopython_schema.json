{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "id": "http://liestudio/schemas/structures_schema.json",
    "title": "Cheminformatics tools",
    "description": "Small molecule cheminformatics tools",
    "type": "object",
    "properties": {
        "pdb_id": {
            "type": "string",
            "description": "RCSB PDB ID"
        },
        "rcsb_file_format": {
            "type": "string",
            "description": "RCSB structure file format",
            "default": "pdb",
            "enum": ["pdb", "xml", "mmCif", "mmtf", "bundle"]
        }
    },
    "required": ["pdb_id"]
}